<div class="row">
	<div class="span7">
			<ul class="nav nav-tabs">
			  <li><%= link_to 'Find Parking', new_search_path %></li>
				<li class="active"><a href="#">Share My Spot</a></li>
			</ul>
		<div class="search-well span7">
			<% if current_user && current_user.leases.count != 0 %>
				<%= render 'form' %>
			<% elsif current_user %>
			<em>You cannot list a parking spot until it is registered. You can register your spot(s)
						 <%= link_to 'here', new_lease_path %>.</em>
			<% else %>
				<%= render 'sessions/login' %>
				<%= button_to "Sign Up!", new_user_path, method: :get, class: "btn btn-large blue" %>
			<% end %>
		</div>
	</div>
	<% if current_user %>
	<div class="span5">
		<div class="custom-well span4 pull-right">
			<h3>Listings</h3>
			<hr>
			<%= render 'listings' %>
		</div>
	</div>
	<% end %>
</div>
<hr>
<%= render 'buildings/participating_buildings' %>
</br>
<hr>
</br>
<%= render 'shared/how_does_it_work' %>
