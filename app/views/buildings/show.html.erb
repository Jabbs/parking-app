<h1><%= @building.name %></h1>
<div class="row">
	<div class="span3 pull-left">
		<%= image_tag(@building.image, size: "100x100") %></br>
		<strong>Location:</strong></br>
		<%= @building.name %></br>
		<%= @building.address %> </br> <%= @building.city %>, 
		<%= @building.state %> <%= @building.zip_code %></br>
		<a href="http://<%= @building.website %>" title="<%= @building.name %>" target="_blank"><%= @building.website %></a>
		</br>
	</div>
</div>

