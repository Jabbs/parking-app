<% if @search2.listings.count != 0 %>

<table class="table table-striped table-condensed table-hover">
	<thead>
	  <tr>
	    <th>Spot</th>
	    <th>Start</th>
	    <th>End</th>
	    <th>Price</th>
	  </tr>
	</thead>
	<tbody>
	<% @search2.listings.each do |listing| %>
	  <tr>
			<td><%= listing.spot.name %></td>
			<td><%= listing.time_slot_start_hour %>, <%= listing.start_date.strftime("%b %e, %Y") %></td>
			<td><%= listing.time_slot_end_hour %>, <%= listing.end_date.strftime("%b %e, %Y") %></td>
			<td><%= number_to_currency(listing.price) %>/hour</td>
			<td><%= link_to 'Reserve', '#' %></td>
		</tr>
	<% end %>
	</tbody>
</table>
<% else %>
<em>No spots listed for <%= @search2.begin_date.strftime("%b %e, %Y") %></em>
<% end %>